RewriteEngine On


RewriteRule ^events/(weekly|special)/edit/(.*)/?$ /events/index.php?page=add_$1&event_id=$2 [L,NC]




RewriteRule ^events/(weekly|special)/add/?$ /events/index.php?page=add_$1 [L,NC]
RewriteRule ^events/(weekly|special)/?$ /events/index.php?page=$1 [L,NC]



RewriteRule ^events/(settings)/?$ /events/index.php?page=$1 [L,NC]





